<!DOCTYPE html>
<html>
<head>
    <title>URL List</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            padding: 10px;
            border: 1px solid #ddd;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>List of URLs</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>URL</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            <% listUrl.forEach(function(item) { %>
                <tr>
                    <td><%= item.name %></td>
                    <td><a href="<%= item.url %>" target="_blank"><%= item.url %></a></td>
                    <td><%= item.description %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
